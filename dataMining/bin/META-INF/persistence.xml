<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="dataMining" transaction-type="RESOURCE_LOCAL">
		<class>model.Composition</class>
		<class>model.EntryInformation</class>
		<class>model.GeneralAnnotation</class>
		<class>model.ProteinAttribute</class>
		<class>model.SequenceAnnotation</class>
		<class>model.TableDeFait</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/dataMining"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="root"/>
			<property name="eclipselink.jdbc.batch-writing" value="JDBC"/>
		</properties>
	
	</persistence-unit>
</persistence>
